\e[1mAVAILABLE EXTENSIONS AND MODULES\e[21m


\e[1mFREE EXTENSIONS\e[21m

<% grouped_extensions[true].each do |extension| -%>
  \e[1m<%= extension[:name] %>\e[21m
  Install with: SUSEConnect -p <%= extension[:activation_code] %>
<% extension[:extensions].each do |additional_extension| -%>
    \e[1m<%= additional_extension[:name] %>\e[21m
    Install with: SUSEConnect -p <%= additional_extension[:activation_code] %>

  <% end -%>

<% end -%>


\e[1mPAID EXTENSIONS\e[21m

<% grouped_extensions[false].each do |extension| -%>
  \e[1m<%= extension[:name] %>\e[21m
  Install with: SUSEConnect -p <%= extension[:activation_code] %> -r \e[32m\e[1mREGCODE\e[21m\e[0m
<% extension[:extensions].each do |additional_extension| -%>

    \e[1m<%= additional_extension[:name] %>\e[21m
    Install with: SUSEConnect -p <%= additional_extension[:activation_code] %> -r \e[32m\e[1mREGCODE\e[21m\e[0m
  <% end -%>

<% end -%>

